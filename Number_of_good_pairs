class Solution {
    public int numIdenticalPairs(int[] nums) {

        int count=0;

        for(int i=0;i<nums.length;i++){ // a step behind
            for(int j=i+1;j<nums.length;j++){ // will start from one step ahead
            if(nums[i]==nums[j]){ //comapring whether we are getting a pair or not 
                count++; // if yes then count the pair and increment it by 1
            } 
        }
        }
        return count;
        
    }
}
